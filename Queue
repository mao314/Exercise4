import java.util.ArrayList;
import java.util.Iterator;

import rp13.search.interfaces.Agenda;


public class Queue<Item_T> implements Agenda<Item_T> {

	private ArrayList<Item_T> queue = new ArrayList<Item_T>();
	
	@Override // how to set iterator?
	public Iterator<Item_T> iterator() {
		return queue.iterator();
	}

	@Override
	public void push(Item_T _item) {
		queue.add(_item);
	}

	@Override
	public boolean isEmpty() {
		if(queue.size() > 0)
			return false;
		else
			return true;
	}

	@Override
	public boolean contains(Item_T _item) {
		for(int i = 0; i < queue.size(); i++) {
			if (queue.get(i).equals(_item) )
				return true;
		}
		return false;	
	}
	
	@Override
	public Item_T pop() {
		Item_T item = queue.get(0);
		queue.remove(0);
		for(int i = 0; i < queue.size(); i++){
			queue.set(i, queue.get(i+1));		
		}
		return item;
	}

}
