import java.util.ArrayList;


public class ExploredStates {

	private ArrayList<Object> explored = new ArrayList<Object>();
	
	public void add(Object _item) {
		explored.add(_item);
	}
		
	public boolean contains(Object _item) {
		boolean checker = false;
		for(int i = 0; i < explored.size(); i++) {
			if (explored.get(i) == _item )
				checker = true;
			else
				checker = false;
		}
		return checker;	
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
